@model IEnumerable<CustomerInfo.Models.tblCustomerWS>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.DropDownList("ZoneTEXT",null,htmlAttributes: new { @class = "form-control"})
