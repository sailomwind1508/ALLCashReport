        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SlpName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustImg)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Zone.ZoneID)
        </td>
